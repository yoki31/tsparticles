{
  "name": "tsparticles-preset-template",
  "version": "1.43.4",
  "private": true,
  "description": "tsParticles template preset",
  "homepage": "https://particles.js.org/",
  "main": "index.js",
  "jsdelivr": "tsparticles.preset.template.min.js",
  "unpkg": "tsparticles.preset.template.min.js",
  "module": "index.js",
  "types": "index.d.ts",
  "type": "module",
  "scripts": {
    "prettify:src": "prettier --write ./*.js",
    "prettify:readme": "prettier --write ./README.md",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "bundle": "webpack --mode production --config webpack.config.cjs",
    "build": "yarn clear:dist && yarn prettify:src && yarn lint && yarn bundle && yarn prettify:readme",
    "clear:dist": "rimraf -f ./dist",
    "prepack": "yarn build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tsparticles/preset-template.git"
  },
  "keywords": [
    "tsparticles",
    "particles",
    "particle",
    "canvas",
    "jsparticles",
    "xparticles",
    "particles-js",
    "particles.js",
    "particles-ts",
    "particles.ts",
    "javascript",
    "animation",
    "web",
    "html5",
    "web-design",
    "webdesign",
    "css",
    "html",
    "css3",
    "animated",
    "background",
    "tsparticles-preset"
  ],
  "author": "Matteo Bruni",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tsparticles/preset-template/issues"
  },
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@babel/eslint-parser": "^7.17.0",
    "babel-loader": "^8.2.5",
    "browserslist": "^4.20.3",
    "copyfiles": "^2.4.1",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "terser-webpack-plugin": "^5.3.1",
    "webpack": "^5.72.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.2"
  },
  "dependencies": {
    "tsparticles-engine": "^2.0.6"
  }
}
