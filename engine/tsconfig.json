{
  "extends": "./tsconfig.base.json",
  "compilerOptions": {
    "module": "commonjs",
    "outDir": "./dist/cjs"
  },
  "typedocOptions": {
    "gaID": "UA-161253125-1",
    "includes": "./markdown",
    "entryPoints": [
      "./src/"
    ],
    "entryPointStrategy": "expand",
    "name": "tsParticles",
    "includeVersion": true,
    "hideGenerator": true,
    "out": "./docs",
    "clarityId": "8q4bxin4tm",
    "carbonServe": "CEAI6KJL",
    "carbonPlacement": "particlesjsorg",
    "validation": {
      "invalidLink": true,
      "notDocumented": true
    }
  }
}
